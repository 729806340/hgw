.hidden {
  display: none !important;
}

.defaultImg {
  width: 100rpx !important;
  height: 100rpx !important;
  border-radius: 50%;
  position: absolute;
  top: 28rpx;
  left: 34rpx;
}

.nobtn {
  border: none !important;
}

.BD {
  position: absolute;
  right: -466rpx;
