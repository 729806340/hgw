page {
  width: 100%;
}

.row {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.header {
  width: 100%;
  background: #fff;
  padding: 24rpx;
  border-top: 1px solid #ddd;
  margin-bottom: 24rpx;
  box-sizing: border-box;
}

.header_block {
  width: 702rpx;
  height: 150rpx;
  background-image: linear-gradient(to right, #df9f51,#fed585);
  box-shadow: 0 1px 2px 0 #fcdfba;
  border-radius: 5px;
  position: relative;
}

.header image {
  width: 100vw;
}

.header_name {
  font-size: 16px;
  color: #fff;
}

.vipStatus {
  width: 98rpx;
  height: 32rpx;
  background: #e09b53;
  border-radius: 8px;
  position: relative;
  color: #fff !important;
  margin-top: 20rpx;
}

.fff {
  font-size: 20rpx;
  color: #fff !important;
  line-height: 32rpx;
  position: absolute;
  left:16rpx;
  z-index: 999;
  text-align: center;
}

.header_conter {
  width: 100%;
  background: #fff;
  padding: 24rpx;
  border-top: 1px solid #ddd;
  margin-bottom: 24rpx;
  position: relative;
}

.login_stu {
  font-size: 15px;
  color: #fff;
  position: absolute;
  top: 60rpx;
  left: 150rpx;
}

.header_login {
  width: 96rpx;
  height: 54rpx;
  border: 1px solid #fff;
  border-radius: 30rpx;
  font-size: 24rpx;
  line-height: 50rpx;
  color: #fff;
  background: transparent;
  position: absolute;
  right: 50rpx;
  top: 52rpx;
  text-align: center;
  line-height: 54rpx;
}

.header_conter::after {
  border: none;
}

.header_conter image {
  width: 92rpx;
  height: 92rpx;
  border-radius: 92rpx;
}

.header_cont {
  color: #fff;
  margin-left: 32rpx;
  position: absolute;
  top: 28rpx;
  left: 118rpx;
}

.myOrder {
  width: 100%;
  background-color: #fff;
  border-radius: 15rpx;
  margin-bottom: 24rpx;
}

.myOrder_t {
  display: flex;
  justify-content: space-between;
  padding: 23rpx;
  font-size: 26rpx;
  border-bottom: 1px solid #eee;
  position: relative;
}

.ricon image {
  width: 26rpx;
  height: 26rpx;
  position: absolute;
  top: 26rpx;
  right: 20rpx;
}

.myOrder_sort {
  display: flex;
  color: #666;
  padding: 0 0 24rpx 0;
  flex-wrap :wrap;
}

.myOrder_sort_item {
  display: flex;
  margin-top: 24rpx;
  flex-direction: column;
  text-align: center;
  width: 20%;
  position: relative;
}

.myOrder_sort_item image {
  width: 50rpx;
  height: 50rpx;
  margin: 0 auto;
}

.myOrder_sort_item .text {
  font-size: 24rpx;
  margin-top: 25rpx;
  color: #666;
}

.myDistribution {
  width: 100%;
  height: 228rpx;
  background-color: #fff;
  border-radius: 15rpx;
  margin-bottom: 24rpx;
}

.myDistribution_sort {
  display: flex;
  color: #666;
  padding: 24rpx 0;
}

.myProperty {
  width: 100%;
  height: 218rpx;
  background-color: #fff;
  border-radius: 15rpx;
  margin-bottom: 24rpx;
}

.myProperty_t {
  display: flex;
  justify-content: space-between;
  padding: 23rpx;
  font-size: 26rpx;
  border-bottom: 2rpx solid #eee;
   position: relative;
}

.myProperty_sort {
  display: flex;
  justify-content: space-around;
  color: #666;
  padding: 24rpx 0;
}

.myProperty_sort_item {
  display: flex;
  flex-direction: column;
  text-align: center;
  width: 20%;
}

.mpCont {
  display: flex;
  flex-direction: row;
  justify-content: center;
  align-items: baseline;
}

.mpNum {
  font-size: 16px;
  color: #fe5a4c;
  letter-spacing: 0;
  text-align: center;
}

.mpClass {
  font-size: 10px;
  color: #fe5a4c;
  letter-spacing: 0;
  text-align: center;
  line-height: 50rpx;
}

.mpAdd {
  font-size: 12px;
  color: #666;
  letter-spacing: 0;
  text-align: center;
  flex: 1;
}

.myProperty_sort_item image {
  width: 44rpx;
  height: 44rpx;
  margin: 0 auto;
}

.tool {
  width: 100%;
  height: 284rpx;
  background-color: #fff;
  border-radius: 15rpx;
  font-size: 26rpx;
}

.tool_sort_item image {
  width: 46rpx;
  height: 46rpx;
  margin: 0 auto;
}
.tool_sort_item .distribution_center {
  width: 48rpx;
  height: 42rpx;
}

.tool_sort_item .distribution_order {
  width: 36rpx;
  height: 40rpx;
}

.tool_sort_item .withdraw_record {
  width: 40rpx;
  height: 38rpx;
}

.tool_sort_item button image {
  width: 46rpx;
  height: 46rpx;
  margin: 0 auto;
}

.tool_sort {
  display: flex;
  flex-wrap: wrap;
  width: 100%;
  padding-top: 24rpx;
}

.tool_sort_item {
  display: flex;
  flex-direction: column;
  font-size: 24rpx;
  width: 25%;
  text-align: center;
  position: relative;
  margin-bottom: 24rpx;
}

.redpoint {
  position: absolute;
  top: 0;
  right: 55rpx;
  background-color: #f00;
  width: 13rpx;
  height: 13rpx;
  border-radius: 50%;
}

.tool_sort_item text {
  line-height: 66rpx;
  color: #666;
}

.tool_sort_item button {
  display: flex;
  flex-direction: column;
}

.tool_sort_item button text {
  line-height: 66rpx;
  color: #666;
  font-size: 25rpx;
}

