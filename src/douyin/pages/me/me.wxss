  padding-left: 16rpx;
}

.seeMore {
  font-size: 12px;
  color: #9e9e9e;
  letter-spacing: 0;
  text-align: right;
  position: absolute;
  right: 60rpx;
}

.seeTitle {
  font-size: 14px;
  color: #222;
  letter-spacing: 0;
  text-align: center;
}
.fixed-y {
