page{
  background: #fff;
}
.titBox{
  width: 100%;
  padding: 20rpx 0;
  display: flex;
  align-items: center;
  justify-content: center;
  background:  #FFFEF6;
}
.titBox image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 16rpx;
}
.titBox view{
  color: #E78B26;
  font-size: 20rpx;
}
.ipt_row{
  width: 100%;
  padding: 24rpx 40rpx;
  box-sizing: border-box;
}
.itemBox{
  width: 100%;
  height: 100rpx;
  border-bottom: 2rpx solid #ddd;
  display: flex;
  align-items: center;
}
.itemBox_add{
  width: 100%;
  border-bottom: 2rpx solid #ddd;
  display: flex;
}
.itemBox_add .left{
  width: 190rpx;
  color: #333;
  font-size: 28rpx;
  line-height: 100rpx;
}
.itemBox .left{
  width: 190rpx;
  color: #333;
  font-size: 28rpx;
  line-height: 100rpx;
}
.itemBox .left text{
  color: red;
  margin-right: 6rpx;
}
.itemBox_add .left text{
  color: red;
  margin-right: 6rpx;
}
.iptBox{
  flex: 1;
  display: flex;
}
.addressBox{
  flex: 1;
}
.addressBox .a{
  display: flex;
  align-items: center;
  width: 100%;
  line-height: 100rpx;
  border-bottom: 2rpx solid #ddd;
}
.addressBox .i{
  line-height: 100rpx;
  font-size: 24rpx;
  color: #333;
  width: 100%;
  height: 100rpx;
}
.addressBox .a image{
  width: 30rpx;
  height: 30rpx;
  margin-right: 10rpx;
}
.addressBox .a view{
  font-size: 24rpx;
  color: #999;
}
.itemBox .ipt{
  height: 100rpx;
  line-height: 100rpx;
  flex: 1;
  font-size: 24rpx;
  color: #333;
}
.wx_phone{
  color: #2CCC60;
  font-size: 24rpx;
  padding: 0 20rpx;
  line-height: 100rpx;
  background: #fff;
}
picker{
  flex:1;
}
.iptcity{
  width: 100%;
  height: 100rpx;
  line-height: 100rpx;
  color: #999;
  font-size: 24rpx;
}
.iptcity_333{
  color: #333;
}
.addressBox .a .col333{
  color: #333;
}
.btnBox{
  width: 640rpx;
  height: 76rpx;
  text-align: center;
  line-height: 76rpx;
  background: #2CCC60;
  color: #fff;
  font-size: 28rpx;
  border-radius: 76rpx;
  margin: 100rpx auto 0;
}