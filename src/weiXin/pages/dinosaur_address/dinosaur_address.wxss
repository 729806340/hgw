page {
  width: 100%;
  overflow-y: auto;
}
.back{
  background: #FFFFFF;
}
.dis{
  /* background-color: rebeccapurple; */
  display: flex;
  align-items: center;
  justify-content: center;
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
}
.picker{
  width: 165rpx;
  height: 78rpx;
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
  /* background: #FFFFFF; */
  border-radius: 10rpx; 
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: center;
  margin-right: 14rpx;
}
.iph{
 margin-right: 26rpx;
}
.picker image{
 width: 18rpx;
 height: 18rpx;
}
/* 搜索框 */
.search_layout {
  flex: 1;
  height: 60rpx;
  display: flex;
  flex-direction: row;
  align-items: center;
  /* padding: 0 24rpx; */
  font-size: 25rpx;
  box-sizing: border-box;
  background: #FFFFFF;
  border: 1px solid #DDDDDD;
  border-radius: 25px;
}
.search_row {
  width: 100%;
  display: flex;
  flex-direction: row;
  line-height: 60rpx;
  align-items: center;
}
.search_txt{
  flex: 1;
  /* padding: 0 24rpx; */
  height: 60rpx;
  line-height: 60rpx;
  /* font-family: PingFangSC-Regular; */
  font-size: 20rpx;
  color: #999999;
  letter-spacing: 0;
}
.icon_search{
   padding: 22rpx ;
   margin-left: 24rpx;
}

.new_address{
  width: 100%;
  padding: 0rpx 28rpx;
  box-sizing: border-box;
  background: #fff;
  display: flex;
  align-items: center;
  justify-content: space-between;
  border-top: 1px solid #EEEEEE;
}

.new_address .l{
  flex: 1;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #999999;
  letter-spacing: 0;
}
.new_address .r{
  display: flex;
  align-items: center;
  color: #24B058;
  font-size: 20rpx;
  width: 140rpx;
}
.new_address .r .icon{
  width: 34rpx;
  height: 34rpx;
  display: block;
  padding: 30rpx 0;
  margin-right: 10rpx;
}





/* 团长 */
.text{
  background: #FFFFFF;
  border-radius: 5px;
  margin: 20rpx ;
}
.new{
  width: 126rpx;
  height: 42rpx;
  background: #2CCC60;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  line-height: 18rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top-left-radius: 5rpx;
}
.ber{
  width: 126rpx;
  height: 42rpx;
  background: #F9A116;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  line-height: 18rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  border-top-left-radius: 5rpx;
}
.cont{
  width: 100%;
  display: flex;
  align-items: flex-start;
  margin-top: 40rpx;
}
.cont image{
 width: 92rpx ;
 height: 92rpx;
 border-radius: 50%;
 margin: 0 32rpx 0 42rpx;
}
.cont .txt{
  flex: 1;
}
.t_1{
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #000000;
  letter-spacing: 0;
}
.t_2{
  margin-right: 68rpx;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #666666;
  letter-spacing: 0;
  margin: 12rpx 22rpx 12rpx 0;
  display: flex;
}
.t_2 .t_2_t{
  width: 100rpx;
}
.t_2 .t_2_b{
  flex: 1;
}
.t_t{
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #F97326;
  letter-spacing: 0;
}
.bon{
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 36rpx;
}
.btn{
  width: 124rpx;
  height: 44rpx;
  background: #2CCC60;
  border-radius: 20px;
  font-family: PingFangSC-Regular;
  font-size: 20rpx;
  color: #FFFFFF;
  letter-spacing: 0;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-right: 52rpx;
}
