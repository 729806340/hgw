page{
  background: #fff;
}
.searchBox{
  width: 100vw;
  position: fixed;
  top: 0rpx;
  left: 0;
  z-index: 9;
  padding: 18rpx 0;
  box-sizing: border-box;
  border-bottom: 2rpx solid #eee;
}
.searchBox_2{
  width: 100%;
  display: flex;
  align-items: center;
  border: 2rpx solid #2CCC60;
  border-radius: 70rpx;
  width: 688rpx;
  height: 70rpx;
  margin: 0 auto;
}
.search_ipt{
  flex: 1;
  line-height: 70rpx;
  height: 70rpx;
  font-size: 24rpx;
  color: #333;
}
.search_img{
  width: 26rpx;
  height: 26rpx;
  padding: 20rpx;
}
.navBox{
  width: 156rpx;
  height: 100vh;
  position: fixed;
  top: 0rpx;
  left: 0;
  padding-top: 106rpx;
  box-sizing: border-box;
}
.nav_item{
  width: 100%;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.nav_item_i{
  width: 128rpx;
  font-size: 24rpx;
  color: #333;
  height: 50rpx;
  line-height: 50rpx;
  text-align: center;
  background: #fff;
  border-radius: 50rpx;
}
.nav_item_i_a{
  background: #2CCC60;
  color: #fff;
}
.goodsBox{
  width: 100vw;
  height: 100vh;
  padding: 108rpx 0 0 158rpx;
  box-sizing: border-box;
}
.goodsList{
  border-left: 2rpx solid #eee;
  width: 100%;
}
.goods_itemBox{
  width: 100%;
  border-bottom: 2rpx solid #eee;
}
.goods_item{
  padding: 36rpx 24rpx;
  box-sizing: border-box;
  display: flex;
  align-items: center;
}
.goods_item_img{
  width: 148rpx;
  height: 148rpx;
  margin-right: 24rpx;
}
.goods_item_info{
  flex: 1;
  height: 148rpx;
  position: relative;
}
.goods_name{
  height: 72rpx;
  font-size: 28rpx;
  color: #333;
  text-overflow: -o-ellipsis-lastline;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  line-clamp: 2;
  -webkit-box-orient: vertical;
}
.goods_spec{
  color: #999;
  font-size: 20rpx;
  line-height: 36rpx;
  height: 36rpx;
}
.goods_price{
  display: flex;
  align-items: flex-end;
}
.goods_price .t{
  font-size: 28rpx;
  color: #FF523A;
}
.goods_price .t text{
  font-size: 24rpx;
  color: #FF523A;
}
.goods_price .t2{
  font-size: 20rpx;
  color: #666;
  text-decoration:line-through;
  margin-left: 4rpx;
}
.stepper{
  width: 148rpx;
  height: 60rpx;
  border: 2rpx solid #999;
  border-radius: 60rpx;
  position: absolute;
  right: 0rpx;
  top: 76rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  z-index: 9;
}
.stepper text{
  line-height: 60rpx;
  font-weight: bold !important;
  text-align: center;
  color: #7bdaaa;
  font-size: 42rpx;
  padding: 15rpx;  
}
.stepper input{
  width: 50rpx;
  height: 26rpx;
  text-align: center;
  font-size: 24rpx; 
}  
.addimg{
  width: 52rpx;
  height: 52rpx;
  position: absolute;
  right: 0rpx;
  top: 82rpx;
  z-index: 9;
}
.addimg_text{
  height: 42rpx;
  position: absolute;
  right: 0rpx;
  top: 92rpx;
  z-index: 9;
  font-size: 24rpx;
  color: #fff;
  padding: 0 12rpx;
  line-height: 42rpx;
  background: #2CCC60;
  border-radius: 42rpx;
}
/* .specBox{

} */
.spec_item{
  width: 100%;
  height:132rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  padding: 36rpx 24rpx;
  box-sizing: border-box;
}
/* .spec_item .l{

} */
.spec_item .l .t{
  font-size: 24rpx;
  color: #999;
  margin: 10rpx 0;
}
.spec_item .l .t2{
  font-size: 28rpx;
  color: #FF523A;
}
.spec_item .l .t2 text{
  font-size: 24rpx;
  color: #FF523A;
}
.stepper_2{
  width: 148rpx;
  height: 60rpx;
  border: 2rpx solid #999;
  border-radius: 60rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.stepper_2 text{
  line-height: 55rpx;
  font-weight: bold !important;
  text-align: center;
  color: #7bdaaa;
  font-size: 42rpx;
  padding: 20rpx;  
}
.stepper_2 input{
  width: 50rpx;
  height: 26px;
  text-align: center;
  font-size: 24rpx; 
}  
.addimg_2{
  width: 52rpx;
  height: 52rpx;
}
.addimg_text_2{
  height: 42rpx;
  font-size: 24rpx;
  color: #fff;
  padding: 0 12rpx;
  line-height: 42rpx;
  background: #2CCC60;
  border-radius: 42rpx;
}
