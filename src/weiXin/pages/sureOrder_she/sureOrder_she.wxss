  align-items: center;
}
.btn_a{
  width: 100%;
  background: #24B058;
  text-align: center;
  height: 90rpx;
  line-height: 90rpx;
  border-radius: 90rpx;
  color: #fff;
  font-size: 28rpx;
  border: 1px solid #24B058;
}
.btn_b{
  display: flex;
  border: 1px solid #24B058;
  height: 90rpx;
  border-radius: 90rpx;
  overflow: hidden;
  background: #24B058;
}
