/* pages/my_commission/my_commission.wxss */
page{
  padding: 20rpx;
  box-sizing: border-box;
}
.content{
  width: 100%;
  height: 308.8rpx;
  background: #fff;
  border-radius: 10rpx;
  padding: 30rpx;
  box-sizing: border-box;
  text-align: center;
  display: flex;
  flex-direction:column;
  justify-content: center;
  align-items: center;
}
.balance{
  font-family: PingFangSC-Medium;
  font-size: 24rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: center;
}
.parice{
  align-self: center;
  margin-top: 12rpx;
  display: flex;
  justify-content: space-between;
  

}
.Y{
    align-self: flex-end;
    font-family: PingFangSC-Medium;
    font-size: 32rpx;
    color: #FF523A;
    letter-spacing: 0;

}
.numb{
  font-family: PingFangSC-Medium;
  font-size: 76rpx;
  color: #FF523A;
  letter-spacing: 0;
  text-align: center;
}

.Card{
   width: 100%;
  background: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  margin-top: 23rpx;
  padding: 0 22rpx;
}
.Card_row{
  width: 100%;
}
.Card .my .left{
  display: flex;
  align-items: center;
}
 .left {
  display: flex;
  align-items: center;
}
.Card .block .left .icon{
  width: 40rpx;
  height: 40rpx;
  margin-right: 20rpx;
}
.Card .block .left .me{
  font-size: 28rpx;
  color: #333;
}
.Card .block .choose{
 width: 30rpx;
 height: 30rpx;
 /* padding: 20rpx; */
}

.record .Le .icon{
  width: 50rpx;
  height: 34rpx;
  border-radius: 50%;
  margin-right: 20rpx;
}
.record .choose{
 width: 18rpx;
 height: 30rpx;
 padding: 20rpx;
}

.Card .my .me{
  font-family: PingFangSC-Medium;
  font-size: 28rpx;
  color: #333333;
  letter-spacing: 0;
  text-align: center;
}





