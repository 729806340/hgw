
view {
  font-weight: 300;
}

.root {
  width: 100vw;
  height: 100vh;
  display: flex;
  flex-direction: column;
  background: #fff;
  overflow: hidden;
}
.yuan{
  flex-direction: row;
  /* height: 74rpx; */
  line-height: 74rpx;
  align-items: center;
  border-bottom: 2rpx solid #f5f5f5;
  display: flex;
  font-size: 26rpx;
  color: #888;
  /* margin-left: 16rpx; */
}
.yuan image{
width: 45rpx;
height: 45rpx;
padding: 0 40rpx;
}

.goods_title_row {
  flex-direction: row;
  height: 74rpx;
  align-items: center;
  padding: 0 20rpx;
  border-bottom: 2rpx solid #f5f5f5;
  display: flex;
}

.img_icon {
  width: 34rpx;
  height: 34rpx;
  margin-left: 30rpx;
}

.shop_name {
  font-size: 26rpx;
  color: #888;
  margin-left: 16rpx;
}

.shop_t1 {
  font-size: 26rpx;
  color: #888;
}

.shop_t2 {
  font-size: 24rpx;
  color: #888;
}

.order_goods_row {
  width: 100vw;
  height: 184rpx;
  display: flex;
  flex-direction: row;
  padding-top: 25rpx;
  box-sizing: border-box;
  align-items: flex-start;
  border-bottom: 2rpx solid #f0f0f0;
}

.order_goods_img {
  width: 136rpx;
  height: 136rpx;
  margin-left: 30rpx;
}

.order_goods_t1 {
  max-width: 450rpx;
  margin-left: 20rpx;
  font-size: 26rpx;
  font-weight: 400;
  color: #000;
}

.ms_img {
  width: 30rpx;
  height: 30rpx;
  vertical-align: middle;
}

.quantity {
  font-size: 24rpx;
  color: #aaa;
  margin-left: 20rpx;
  margin-top: 20rpx;
}

.column {
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
}

.order_goods_col {
  display: flex;
  flex-direction: column;
  align-items: flex-end;
  padding-right: 30rpx;
}

.order_goods_t2 {
  font-size: 30rpx;
  font-weight: 400;
  color: #000;
  margin-bottom: 30rpx;
}

.goods_bottom_row {
  width: 100vw;
  height: 80rpx;
  display: flex;
  box-sizing: border-box;
  padding-right: 30rpx;
  align-items: center;
  flex-direction: row;
  border-bottom: #f5f5f5 solid 2rpx;
}

.goods_bottom_row_t1 {
  font-size: 24rpx;
  color: #555;
  margin-left: 15rpx;
}

.goods_bottom_row_t2 {
  font-size: 24rpx;
  color: #fa7e36;
  margin-left: 15rpx;
}

.row2 {
  display: flex;
  flex-direction: row;
  padding-top: 20rpx;
  align-items: center;
  padding-right: 20rpx;
  border-bottom: 20rpx solid #f0f0f0;
  padding-bottom: 20rpx;
}

.pay_type {
  color: #333;
  font-size: 24rpx;
  margin-left: 30rpx;
}

.row22 {
  display: flex;
  flex-direction: row;
  padding: 15rpx 0;
}

.btn_no {
  height: 56rpx;
  width: 160rpx;
  font-size: 24rpx;
  margin-left: 16rpx;
  color: #444;
  border-radius: 10rpx;
  background: #fff;
  border: solid #ccc 2rpx;
  line-height: 56rpx;
  text-align: center;
}

.btn_ok {
  height: 56rpx;
  width: 160rpx;
  font-size: 24rpx;
  margin-left: 16rpx;
  color: #fff;
  border-radius: 10rpx;
  background: #fa7e36;
  line-height: 56rpx;
  text-align: center;
}

.scroll {
  width: 100vw;
  height: 100vh;
  padding-top: 82rpx;
  box-sizing: border-box;
}

.info_b_t1 {
  font-size: 28rpx;
  color: #999;
  width: 100vw;
  background: #fff;
  padding-top: 20rpx;
  padding-left: 30rpx;
}

.ba_txt {
  font-size: 28rfpx;
  color: #222;
}

.info_b_t2 {
  font-size: 28rpx;
  color: #999;
  width: 100vw;
  background: #fff;
  padding-top: 20rpx;
  border-bottom: 2rpx solid #f0f0f0;
  padding-bottom: 20rpx;
  padding-left: 30rpx;
}

.column_null {
  position: fixed;
  align-self: center;
  width: 100vw;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.column_null_img {
  width: 200rpx;
  height: 200rpx;
  margin-top: 250rpx;
}

.column_null_txt {
  font-size: 26rpx;
  color: #999;
  margin-top: 40rpx;
}
.navBox{
  width: 100vw;
  display: flex;
  align-items: center;
  height: 82rpx;
  border-bottom: 2rpx solid #ddd;
  background: #fff;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 9;
}
.nav_item{
  flex: 1;
  line-height: 82rpx;
  border-bottom: 4rpx solid #fff;
  text-align: center;
  font-size: 26rpx;
  color: #333;
}
.nav_item_a{
  border-color: #2CCC60;
  color: #2CCC60;
}